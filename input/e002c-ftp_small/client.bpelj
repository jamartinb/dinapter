	<invoke operation="user">
		<input part="username"/>
	</invoke>
	<invoke operation="password">
		<input part="pass"/>
	</invoke>
	<invoke operation="download">
		<input part="filename"/>
	</invoke>
	<receive operation="data">
		<input part="data"/>
	</receive>
	<exit/>
<!-- </sequence> -->

</process>
